{
    "suggest.enablePreview": true,
    "suggest.echodocSupport": true,
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "always",
    "suggest.completionItemKindLabels": {
        "method": "ùíé",
        "function": "Ôûî",
        "file": "ÔÄñ",
        "class": "ÔÄÑ",
        "folder": "Óóæ",
        "text": "ùêì",
        "snippet": "‚ö°Ô∏é",
        "keyword": "ÔÇÑ",
        "operator": "ÓûÄ",
        "variable": "ùí±",
        "color": "Ôíè",
        "value": "ÔÖö",
        "module": "Ôíá",
        "reference": "ÔÄ≠",
        "enum": "‚ú¶",
        "constant": "ùíû",
        "unit": "ùí∞",
        "interface": "ÔÇä",
        "struct": "Óòã",
        "enumMember": "Ôâõ",
        "default": " "
    },
    "diagnostic.infoSign": "!",
    "diagnostic.hintSign": "?",
    "diagnostic.warningSign": "‚óè",
    "diagnostic.errorSign": "√ó",
    "diagnostic.highlightOffset": 1000,
    "diagnostic.virtualText": true,
    "diagnostic.checkCurrentLine": true,
    "codeLens.enable": true,
    "signature.target": "echo",
    "coc.preferences.hoverTarget": "float",
    "git.realtimeGutters": false,
    "git.addedSign.text": "‚ñé",
    "git.changedSign.text": "‚ñé",
    "git.removedSign.text": "‚ó¢",
    "git.topRemovedSign.text": "‚ó•",
    "git.changeRemovedSign.text": "‚ó¢",
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "languageserver": {
        "cquery": {
            "command": "cquery",
            "args": [
                "--log-file=/tmp/cq.log"
            ],
            "filetypes": [
                "c",
                "cpp"
            ],
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cacheDirectory": "/tmp/cquery"
            }
        },
        "digestif": {
            "command": "digestif",
            "filetypes": [
                "tex",
                "plaintex",
                "context"
            ]
        },
        "golang": {
            "command": "gopls",
            "filetypes": [ "go" ]
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": [
                "lua"
            ]
        },
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": ["swift"],
        },
        "python": {
            "command": "python3",
            "args": [
                "-mpyls",
                "-vv",
                "--log-file",
                "/tmp/lsp_python.log"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "python"
            ],
            "settings": {
                "pyls": {
                    "enable": true,
                    "trace": {
                        "server": "verbose"
                    },
                    "commandPath": "",
                    "configurationSources": [
                        "pycodestyle"
                    ],
                    "plugins": {
                        "jedi_completion": {
                            "enabled": true
                        },
                        "jedi_hover": {
                            "enabled": true
                        },
                        "jedi_references": {
                            "enabled": true
                        },
                        "jedi_signature_help": {
                            "enabled": true
                        },
                        "jedi_symbols": {
                            "enabled": true,
                            "all_scopes": true
                        },
                        "mccabe": {
                            "enabled": true,
                            "threshold": 15
                        },
                        "preload": {
                            "enabled": true
                        },
                        "pycodestyle": {
                            "enabled": true
                        },
                        "pydocstyle": {
                            "enabled": false,
                            "match": "(?!test_).*\\.py",
                            "matchDir": "[^\\.].*"
                        },
                        "pyflakes": {
                            "enabled": true
                        },
                        "rope_completion": {
                            "enabled": true
                        },
                        "yapf": {
                            "enabled": true
                        }
                    }
                }
            }
        }
    }
}
