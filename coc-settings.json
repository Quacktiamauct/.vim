{
    "suggest.enablePreview": true,
    "suggest.echodocSupport": true,
    "suggest.floatEnable": true,
    "suggest.autoTrigger": "trigger",
    "suggest.noselect": true,
    "suggest.completionItemKindLabels": {
        "method": "ùíé",
        "function": "Ôûî",
        "file": "ÔÖõ",
        "class": "ÔÄÑ",
        "folder": "Óóæ",
        "text": "ùêì",
        "snippet": "‚ö°Ô∏é",
        "keyword": "ÔêΩ",
        "operator": "ÓûÄ",
        "variable": "Ôôñ",
        "color": "Ôùª",
        "value": "Ô¢ü",
        "module": "Ô£ñ",
        "reference": "ÔÄ≠",
        "enum": "‚ú¶",
        "constant": "Ô£æ",
        "unit": "Ôëµ",
        "interface": "ÔÇä",
        "struct": "Óòã",
        "enumMember": "Ô¨∑",
        "default": " "
    },
    "suggest.snippetIndicator": "Ôëú",
    "diagnostic.infoSign": "ÔÅö",
    "diagnostic.hintSign": "Ô†¥",
    "diagnostic.warningSign": "ÔÑë",
    "diagnostic.errorSign": "ÔÅó",
    "diagnostic.highlightOffset": 1000,
    "diagnostic.virtualText": true,
    "diagnostic.refreshAfterSave": false,
    "diagnostic.checkCurrentLine": true,
    "codeLens.enable": true,
    "signature.target": "float",
    "coc.preferences.hoverTarget": "float",
    "git.realtimeGutters": false,
    "git.addedSign.text": "‚ñé",
    "git.changedSign.text": "‚ñé",
    "git.removedSign.text": "‚ó¢",
    "git.topRemovedSign.text": "‚ó•",
    "git.changeRemovedSign.text": "‚ó¢",
    "git.addedSign.hlGroup": "DiffAddGutter",
    "git.changedSign.hlGroup": "DiffChangeGutter",
    "git.removedSign.hlGroup": "DiffDeleteGutter",
    "git.topRemovedSign.hlGroup": "DiffDeleteGutter",
    "git.changeRemovedSign.hlGroup": "DiffChangeGutter",
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "list.insertMappings": {
        "<C-c>": "do:exit"
    },
    "tabnine.disable_filetypes": ["tex", "markdown", "json"],
    "tabnine.priority": 80,
    "languageserver": {
        "cquery": {
            "command": "cquery",
            "args": [
                "--log-file=/tmp/cq.log"
            ],
            "filetypes": [
                "c",
                "cpp"
            ],
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cacheDirectory": "/tmp/cquery"
            }
        },
        "digestif": {
            "command": "digestif",
            "filetypes": [
                "tex",
                "plaintex",
                "context"
            ]
        },
        "lua": {
            "command": "lua-lsp",
            "filetypes": [
                "lua"
            ]
        },
        "bash": {
            "command": "bash-language-server",
            "args": [
                "start"
            ],
            "filetypes": [
                "sh"
            ],
            "ignoredRootPaths": [
                "~"
            ]
        },
        "ocaml": {
            "command": "ocaml-language-server",
            "args": ["--stdio"],
            "filetypes": ["ocaml", "reason"]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": ["swift"]
        },
        "python": {
            "command": "python3",
            "args": [
                "-mpyls",
                "-vv",
                "--log-file",
                "/tmp/lsp_python.log"
            ],
            "trace.server": "verbose",
            "filetypes": [
                "python"
            ],
            "settings": {
                "pyls": {
                    "enable": true,
                    "trace": {
                        "server": "verbose"
                    },
                    "commandPath": "",
                    "configurationSources": [
                        "pycodestyle"
                    ],
                    "plugins": {
                        "jedi_completion": {
                            "enabled": true
                        },
                        "jedi_hover": {
                            "enabled": true
                        },
                        "jedi_references": {
                            "enabled": true
                        },
                        "jedi_signature_help": {
                            "enabled": true
                        },
                        "jedi_symbols": {
                            "enabled": true,
                            "all_scopes": true
                        },
                        "mccabe": {
                            "enabled": true,
                            "threshold": 15
                        },
                        "preload": {
                            "enabled": true
                        },
                        "pycodestyle": {
                            "enabled": true
                        },
                        "pydocstyle": {
                            "enabled": false,
                            "match": "(?!test_).*\\.py",
                            "matchDir": "[^\\.].*"
                        },
                        "pyflakes": {
                            "enabled": true
                        },
                        "rope_completion": {
                            "enabled": true
                        },
                        "yapf": {
                            "enabled": true
                        }
                    }
                }
            }
        }
    }
}
